@model MackPortfolio.Models.ActionLayoutModel

<script src="http://code.jquery.com/ui/1.11.1/jquery-ui.js" type="text/javascript"></script>
<script src="http://code.jquery.com/color/jquery.color.plus-names-2.1.2.min.js" type="text/javascript"></script>

@Scripts.Render("~/libs/knockout")
<script src="http://cdnjs.cloudflare.com/ajax/libs/knockout-validation/1.0.2/knockout.validation.js" type="text/javascript"></script>
<script src="http://cdnjs.cloudflare.com/ajax/libs/knockout.mapping/2.4.1/knockout.mapping.js" type="text/javascript"></script>

<script src="http://rniemeyer.github.io/knockout-kendo/js/knockout-kendo.min.js" type="text/javascript"></script>

@if (Request.IsIE())
{
    if (Request.CompareVersion(x => x < 9))
    {
        <script src="@Url.Content("~/Scripts/rem-fix.js")" type="text/javascript"></script>
        if (Request.CompareVersion(x => x < 7))
        {
            <script src="http://cdnjs.cloudflare.com/ajax/libs/dd_belatedpng/0.0.8/dd_belatedpng.min.js" type="text/javascript"></script>
            <script type="text/javascript">DD_belatedPNG.fix('img, .png_bg');</script>
        }
    }
}
@Scripts.Render("~/libs/ko", "~/libs/site","~/libs/foundation")
<script type="text/javascript">
    if ('undefined' === typeof main) {
        var main = document.getElementById("mainapp");
    }
    main.isDebugMode = Boolean('@Model.isDebugMode');
    $(document).foundation();
</script>
